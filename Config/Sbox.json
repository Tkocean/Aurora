 {
  "log": {
    "disabled": true,
    "level": "error",
    "output": "debug.log"
  },
  "dns": {
    "servers": [
      {
        "tag": "foreign",
        "address": "https://8.8.8.8/dns-query",
        "detour": "Server"
      },
      {
        "tag": "fakedns",
        "address": "fakeip"
      },
      {
        "tag": "internal",
        "address": "udp://223.5.5.5",
        "detour": "Direct"
      }
    ],
    "rules": [
      {
        "outbound": "any",
        "server": "internal"
      },
      {
        "rule_set": "private",
        "domain_keyword": ["push","frp","ntp","time"],
        "server": "internal"
      },
      {
        "query_type": ["A","AAAA"],
        "server": "fakedns",
        "rewrite_ttl": 0,
        "disable_cache": true
      }
    ],
    "fakeip": {
      "enabled": true,
      "inet4_range": "198.18.0.0/15",
      "inet6_range": "fc00::/18"
    },
    "strategy": "prefer_ipv4",
    "reverse_mapping": false,
    "independent_cache": true
  },
  "inbounds": [
    {
      "type": "tproxy",
      "tag": "tproxy-in",
      "listen": "::",
      "listen_port": 26
    }
  ],
  "outbounds": [
    {
      "type": "direct",
      "tag": "Direct"
    },
    {
      "tag": "Server",
      "type": "shadowsocks"
    }
  ],
  "route": {
    "rule_set": [
      {
        "type": "remote",
        "tag": "private",
        "format": "binary",
        "url": "https://testingcf.jsdelivr.net/gh/MetaCubeX/meta-rules-dat@sing/geo/geosite/private.srs",
        "download_detour": "Direct",
        "update_interval": "6d"
      },
      {
        "type": "remote",
        "tag": "google",
        "format": "binary",
        "url": "https://testingcf.jsdelivr.net/gh/MetaCubeX/meta-rules-dat@sing/geo/geosite/google.srs",
        "download_detour": "Direct",
        "update_interval": "6d"
      },
      {
        "type": "remote",
        "tag": "games",
        "format": "binary",
        "url": "https://testingcf.jsdelivr.net/gh/MetaCubeX/meta-rules-dat@sing/geo/geosite/category-games@cn.srs",
        "download_detour": "Direct",
        "update_interval": "6d"
      },
      {
        "type": "remote",
        "tag": "apple",
        "format": "binary",
        "url": "https://testingcf.jsdelivr.net/gh/MetaCubeX/meta-rules-dat@sing/geo/geosite/apple.srs",
        "download_detour": "Direct",
        "update_interval": "6d"
      },
      {
        "type": "remote",
        "tag": "cn-domain",
        "format": "binary",
        "url": "https://testingcf.jsdelivr.net/gh/MetaCubeX/meta-rules-dat@sing/geo/geosite/cn.srs",
        "download_detour": "Direct",
        "update_interval": "6d"
      },
      {
        "type": "remote",
        "tag": "cn-ip",
        "format": "binary",
        "url": "https://testingcf.jsdelivr.net/gh/MetaCubeX/meta-rules-dat@sing/geo/geoip/cn.srs",
        "download_detour": "Direct",
        "update_interval": "6d"
      }
    ],
    "rules": [
      {
        "action": "sniff"
      },
      {
        "type": "logical",
        "mode": "or",
        "rules": [
          {
            "port": 53
          },
          {
            "protocol": "dns"
          }
        ],
        "action": "hijack-dns"
      },
      {
        "rule_set": "google",
        "outbound": "Server"
      },
      {
        "rule_set": ["private","apple","games","cn-domain"],
        "domain_keyword": ["push","frp","ntp","time"],
        "outbound": "Direct"
      },
      {
        "rule_set": "cn-ip",
        "outbound": "Direct"
      },
      {
        "ip_is_private": true,
        "outbound": "Direct"
      }
    ],
    "find_process": false,
    "final": "Server"
  },
  "experimental": {
    "cache_file": {
      "enabled": true,
      "store_rdrc": false,
      "store_fakeip": false,
      "path": "/cache/Sbox.db"
    }
  }
}
