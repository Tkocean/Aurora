{
  "log": {
    "disabled": true,
    "level": "error",
    "output": "Sbox.log",
    "timestamp": false
  },
  "experimental": {
    "cache_file": {
      "enabled": true,
      "store_fakeip": false,
      "path": "Sbox.db"
    }
  },
  "dns": {
    "strategy": "prefer_ipv4",
    "independent_cache": true,
    "reverse_mapping": false,
    "servers": [
      {
        "tag": "internal",
        "type": "udp",
        "server": "223.5.5.5"
      },
      {
        "tag": "fakedns",
        "type": "fakeip",
        "inet4_range": "198.18.0.0/15",
        "inet6_range": "fc00::/18"
      }
    ],
    "rules": [
      {
        "query_type": ["A","AAAA"],
        "server": "fakedns",
        "rewrite_ttl": 0,
        "disable_cache": true
      },
      {
        "outbound": "internal",
        "server": "internal"
      }
    ]
  },
  "inbounds": [
    {
      "type": "tproxy",
      "tag": "tproxy-in",
      "listen": "::",
      "listen_port": 26
    }
  ],
  "outbounds": [
    {
      "type": "direct",
      "tag": "Direct"
    }
  ],
  "route": {
    "final": "Direct",
    "default_domain_resolver": "internal",
    "rule_set": [
      {
        "tag": "cn-ip",
        "type": "remote",
        "url": "https://testingcf.jsdelivr.net/gh/MetaCubeX/meta-rules-dat@sing/geo/geoip/cn.srs",
        "download_detour": "Direct",
        "update_interval": "6d"
      },
      {
        "tag": "cn-domain",
        "type": "remote",
        "url": "https://testingcf.jsdelivr.net/gh/MetaCubeX/meta-rules-dat@sing/geo/geosite/cn.srs",
        "download_detour": "Direct",
        "update_interval": "6d"
      },
      {
        "tag": "private",
        "type": "remote",
        "url": "https://testingcf.jsdelivr.net/gh/MetaCubeX/meta-rules-dat@sing/geo/geosite/private.srs",
        "download_detour": "Direct",
        "update_interval": "6d"
      }
    ],
    "rules": [
      {
        "action": "sniff"
      },
      {
        "type": "logical",
        "mode": "or",
        "rules": [
          {"port": 53},
          {"protocol": "dns"}
        ],
        "action": "hijack-dns"
      },
      {
        "rule_set": ["private", "cn-domain"],
        "outbound": "Direct"
      },
      {
        "rule_set": "cn-ip",
        "outbound": "Direct"
      },
      {
        "ip_is_private": true,
        "outbound": "Direct"
      }
    ],
    "find_process": false,
    "auto_detect_interface": false
  }
}